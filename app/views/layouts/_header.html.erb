 <header class="navbar navbar-fixed-top navbar-inverse">
    <div class="container">
      <%= link_to "Kitcarson Technology", root_path, id: "logo" %>
        <nav>
          <ul class="nav navbar-nav pull-right">
            <li><%= link_to "Home",   root_path %></li>
            <li><%= link_to "Help",   help_path %></li>

            <% if logged_in? %>
              <li> <%= link_to "Invoices", invoices_path  %> </li>
              <li class="dropdown">
                <a href = "#", class= "dropdown-toggle", data-toggle= "dropdown">
                  Account <b class="caret"></b>
                </a>
                <ul class="dropdown-menu">
                  <% @cuurrent_user = current_user %>
                  <li><%= link_to "Profile", user_path(@current_user[:user_id]) %></li>
                  <li><%#= link_to "Settings", edit_user_path(current_user) %><a href="#">Settings</a></li>
                  <li class="divider"></li>
                  <li><%= link_to "Log out", logout_path, method: "delete" %><%#= link_to "Log in", login_path %></li>
                </ul>
              </li>

            <% else %>
              <li><%= link_to "Log in", login_path %></li>
            <% end  %>
          </ul>
        </nav>
    </div>
 </header>
