 <header class="navbar navbar-fixed-top navbar-custom">
    <div class="container">
      <div class="logo">
      <%=link_to image_tag('Logo_Kit-1.png'), root_path %>
     </div>
        <nav>
          <ul class="nav navbar-nav pull-right">


            <% if logged_in? %>
              <li><%= link_to "Dashboard",   root_path %></li>
              <li> <%= link_to "Billing", invoices_path  %> </li>
              <li><%= link_to "Payments",   payments_path %></li>

              <li class="dropdown">
                <a href = "#", class= "dropdown-toggle", data-toggle= "dropdown">
                  Account <b class="caret"></b>
                </a>
                <ul class="dropdown-menu">
                  <% @current_user = current_user %>
                  <li><%= link_to "Profile", user_path(@current_user[:user_id]) %></li>
                  <li><%= link_to "Settings", edit_user_path(@current_user[:user_id]) %></li>
                  <li class="divider"></li>
                  <li><%= link_to "Log out", logout_path, method: "delete" %><%#= link_to "Log in", login_path %></li>
                </ul>
              </li>

            <% else %>
              <li><%= link_to "Log in", login_path %></li>
            <% end  %>
          </ul>
        </nav>
    </div>
 </header>
